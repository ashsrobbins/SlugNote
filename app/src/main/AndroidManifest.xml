<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="slugnote.com.slugnote">

    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity_Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".coursesFragment"
        android:label="courses">
        </activity>

        <activity android:name=".notesFragment"
        android:label="notes">
        </activity>

        <activity android:name=".uploadFragment"
            android:label="upload">
        </activity>

        <activity android:name=".submitFragment"
            android:label="submit">
        </activity>

        <activity android:name=".myNotebook"
            android:label="mynotebook">
        </activity>

        <activity android:name=".searchFragment"
            android:label="search">

        </activity>
        <activity android:name=".Register"
            android:label="register">

        </activity>
        <activity android:name=".categoryFragment"
            android:label="category">

        </activity>

        <activity android:name=".noteviewFragment"
            android:label="noteview">

        </activity>





        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>
